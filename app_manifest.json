{
  "display_information": {
    "name": "jollyrogerbay",
    "description": "@jollyroger182's bay",
    "background_color": "#004492"
  },
  "features": {
    "bot_user": {
      "display_name": "jollyrogerbay",
      "always_online": true
    }
  },
  "oauth_config": {
    "redirect_urls": ["https://localhost:16483"],
    "scopes": {
      "user": [
        "dnd:read",
        "dnd:write",
        "users.profile:read",
        "users.profile:write",
        "chat:write"
      ],
      "bot": [
        "app_mentions:read",
        "canvases:read",
        "channels:history",
        "channels:join",
        "channels:manage",
        "channels:read",
        "channels:write.invites",
        "channels:write.topic",
        "chat:write",
        "chat:write.customize",
        "chat:write.public",
        "dnd:read",
        "emoji:read",
        "files:read",
        "files:write",
        "groups:history",
        "groups:read",
        "groups:write",
        "groups:write.invites",
        "groups:write.topic",
        "im:history",
        "im:read",
        "im:write",
        "im:write.topic",
        "metadata.message:read",
        "mpim:history",
        "mpim:read",
        "mpim:write",
        "mpim:write.topic",
        "pins:read",
        "pins:write",
        "reactions:read",
        "reactions:write",
        "usergroups:read",
        "usergroups:write",
        "users.profile:read",
        "users:read",
        "users:read.email",
        "users:write",
        "commands",
        "links:read",
        "links:write"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "request_url": "https://jollyrogerbay-switch.davidwhy.me/redir/slack/events",
      "bot_events": [
        "app_mention",
        "member_joined_channel",
        "member_left_channel",
        "function_executed"
      ]
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false,
    "hermes_app_type": "remote",
    "function_runtime": "remote"
  },
  "functions": {
    "test-step": {
      "title": "Test Workflow Steps",
      "description": "What does this even do lol",
      "input_parameters": {
        "num": {
          "type": "integer",
          "title": "A Number",
          "description": "Some number, doesn't matter",
          "is_required": true,
          "hint": "What's a hint?",
          "name": "num"
        }
      },
      "output_parameters": {
        "outnum": {
          "type": "number",
          "title": "Output number!",
          "description": "The output.",
          "is_required": true,
          "hint": "A hint again?",
          "name": "outnum"
        }
      }
    },
    "user-channel-ping": {
      "title": "Channel ping with user token",
      "description": "@channel but with your user token",
      "input_parameters": {
        "channel": {
          "type": "slack#/types/channel_id",
          "title": "Channel",
          "description": "Which channel to ping",
          "is_required": true,
          "name": "channel"
        },
        "content": {
          "type": "slack#/types/rich_text",
          "title": "Content",
          "description": "Optional message content",
          "is_required": false,
          "hint": "What to say after the @channel",
          "name": "content"
        }
      },
      "output_parameters": {}
    },
    "user-here-ping": {
      "title": "Here ping with user token",
      "description": "@here but with your user token",
      "input_parameters": {
        "channel": {
          "type": "slack#/types/channel_id",
          "title": "Channel",
          "description": "Which channel to ping",
          "is_required": true,
          "name": "channel"
        },
        "content": {
          "type": "slack#/types/rich_text",
          "title": "Content",
          "description": "Optional message content",
          "is_required": false,
          "hint": "What to say after the @here",
          "name": "content"
        }
      },
      "output_parameters": {}
    }
  }
}
